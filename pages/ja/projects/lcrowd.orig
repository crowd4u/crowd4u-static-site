function showContributors(primary_group_id, prj_id, display_nums, output_destination_id_name, destination_header_id_name, is_finished) {
  // 非同期通信
  // ランキングページからスクレイピング
  $.ajax({  
// ランキングページではなくまとめページから取ってくる場合，10件しか取れない．また，HTMLの構造も変わる
//  url:  'http://crowd4u.org/ja/projects',
    url:  'https://crowd4u.org/ja/projects/ranking',
    type: 'GET',
  }).success(function(data){

    // httpでアクセスしているときはこれで良い
//    var contributors  = $(data).find('#p'+prj_id+' + div.proj .sub_member ul li');
    var contributors = $(data).find('#group'+primary_group_id+' + table tr td:nth-child(2)');

    // httpsでアクセスしているときはレスポンスが変わるので取得しなおし
    if (contributors.length == 0){
//      var contributors = $(data.results[0]).find('#p'+prj_id+' + div.proj .sub_member ul li');
      var contributors = $(data.results[0]).find('#group'+primary_group_id+' + table tr td:nth-child(2)');
    }

    // n名中m名の表示．終わっていない場合は最新ではないランキングの可能性があるため暫定をつける
    if (is_finished == "TRUE"){
      $("#"+destination_header_id_name).append('(' +contributors.length + '名中タスク実施数の上位' + display_nums + '名)');
//      $("#"+destination_header_id_name).append('(タスク数上位' + display_nums + '名)');
    }else {
      $("#"+destination_header_id_name).append('(' +contributors.length + '名中タスク実施数の上位' + display_nums + '名(暫定))');
//      $("#"+destination_header_id_name).append('(タスク数上位' + display_nums + '名(暫定))');
    }

    // 貢献者を出力
    contributors.each(function(index){
      $("#"+output_destination_id_name)
        .append('<li style="display:inline-block; width:210px; ">' + $(this).text() + '</li>');
          if(index == (display_nums -1)){ // display_nums個出力したら
            return false; // break;
          }
      });

