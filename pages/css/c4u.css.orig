/*--------------------*/
/* RESPONSIVE STYLE */
/*--------------------*/

@media (max-width: 767px){
	img.mu-footer-logo {
		width: 50%;
	}

	.mu-prjTitle-area { padding: 50px 0; }
	.mu-prjTitle-area h1 { font-size: 30px; }

	.cheer {
		margin: 1em 0;
		text-align: left;
	}

	.mu-prjForm_aboutPrj-area .mu-title,
	.mu-prjForm_aboutTsk-area .mu-title,
	.mu-contact-content { padding: 5%; }

	.mu-contact-content p { margin-top: 2em; }

	.mu-prjForm_aboutPrj-area .mu-contact-content h3:first-child ,
	.mu-prjForm_aboutTsk-area .mu-contact-content h3:first-child {
		margin-top: 0;
	}

	.mu-prjForm_end-area .row.btn-area .mu-btn-cancel,
	.mu-prjForm_end-area .row.btn-area .mu-btn-primary {
		padding: 15px 20px;
		
	}

	.mu-prjForm_end-area .row.btn-area .col-md-8 { margin-top: -1.5em; }
}

@media (max-width: 640px){

	.mu-single-slide-content a {
		width: 65%;
		margin: 0 auto;
	}

	.mu-single-slide-content h1 {
		font-size: 18px;
		margin-bottom: 5px;
	}

	.mu-single-slide-content p {
		font-size: 12px;
		margin-bottom: .8rem;
	}

	.mu-btn-primary-hero {
		padding: 10px 35px;
		font-size: 12px;
	}
}


@media (max-width: 480px) {

	.mu-single-slide-content a.mu-primary-btn {
		display: block;
	}

}

@media (max-width: 1199px) {
	.mu-single-team-img { width: 100%; }
	.mu-single-team-content{ width:100%; }
}



