/* forResearchers,forEngineers,*/
/* let's begin                 */
/*-----------------------------*/

.mu-forResearchers-content,
.mu-forEngineers-content,
.mu-begin-content {
	display: inline;
	float: left;
	text-align: center;
	width: 100%;
}

#mu-forResearchers {
		background-image: url("/images/bg_forreseachers.jpg");
}

#mu-forEngineers {
	background-image: url("/images/bg_forengineers.jpg");
	border-top: 1px solid #ffffff;
}

#mu-begin {
		background-image: url("/images/bg_register.jpg");
}

#mu-forResearchers,
#mu-forEngineers,
#mu-begin {
	background-position: center center;
	background-size: cover;
	display: inline;
	float: left;
	position: relative;
	width: 100%;
}

#mu-forResearchers::before,
#mu-forEngineers::before,
#mu-begin::before {
	background: #000000;

}




#mu-forResearchers::before,
#mu-forEngineers::before,
#mu-begin::before {
	bottom: 0;
	content: '';
	left: 0;
	top: 0;
	right: 0;
	position: absolute;
	opacity: 0.4;
	z-index: 10;
}


.mu-forResearchers-area,
.mu-forEngineers-area,
.mu-begin-area {
	display: inline;
	float: left;
	position: relative;
	color: #ffffff;
	width: 100%;
