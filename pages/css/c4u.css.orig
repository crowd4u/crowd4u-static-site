/*headerの高さのずれの修正*/
.mu-logo img {
	margin-top: 5px;
	width: 200px;
	margin-bottom: 5px;
}

.mu-logo span {
	margin-top: -3px;
	color: #999999;
	display: block;
	font-size: 10px;
}


.mu-navbar {
	padding: 0;
	box-shadow: 0px 2px gray; /*headerの下に薄く下線*/
}

.mu-navbar-nav li a {
	border-radius: 3px;
}

.mu-navbar-nav li a {
	color: #666666;
}

.mu-navbar-nav li.active a,
.mu-navbar-nav li a:hover,
.mu-navbar-nav li a:focus {
	background-color: #fff;
	color: #f67b30;
}

.username {
	font-size: 12px;
	display: inline;
	padding: 18px 0 18px 18px;
	border-left: 1px solid #999999;
}

.username span {
	font-size: 16px;
	font-weight: bold;
}



.mu-btn {
	background-color: #ffffff;
	padding: 15px 70px;
	border: 1px solid #6e5995;
	border-radius: 3px;
	color: #6e5995;
	opacity: 0.7;
	text-transform: uppercase;

}

.mu-btn-cancel {
	background-color: #ffffff;
	padding: 15px 70px;
	border: 1px solid #666666;
	border-radius: 3px;
	color: #666666;
	opacity: 0.7;
	text-transform: uppercase;

}


.mu-btn:hover,
.mu-btn:focus {
	background-color: #9c88bb;
	color: #ffffff;
	opacity: 1.0;
	transition: .5s;
}

.mu-btn-cancel:hover,
.mu-btn-cancel:focus {
	background-color: #e5e5e5;
	color: #666666;
	opacity: 1.0;
	transition: .5s;
}

.nav-item .mu-btn_s {
	background-color: #ffffff;
	padding: .3rem 1rem;
	border: 1px solid #6e5995;
	border-radius: 3px;
	color: #6e5995;
	opacity: 0.7;
	text-transform: uppercase;

}

.nav-item .mu-btn_s:hover,
.nav-item .mu-btn_s:focus {
	background-color: #9c88bb;
	color: #ffffff;
	opacity: 1.0;
	transition: .5s;
}

.mu-btn-primary {
	background-color: #f67b30;
	border: 1px solid #f67b30;
	color: #ffffff;
	padding: 15px 70px;
	border-radius: 3px;
	text-transform: uppercase;
}

.mu-btn-primary-hero {
	padding: 15px 70px;
}

.mu-btn-primary:hover,
.mu-btn-primary:focus {
	background-color: #ffb04c;
	border: 1px solid #f67b30;
	color: #ffffff;
	transition: .5s;
}

.mu-btn-secondary {
	background-color: #b869a1;
	border: 1px solid #b869a1;
	border-radius: 3px;
	color: #ffffff;
	display: inline-block;
	padding: 10px 50px;
	text-align: center;
	text-transform: uppercase;
	margin-bottom: 20px;
}

.mu-btn-secondary:hover,
.mu-btn-secondary:focus {
	background-color: #efefef;
	color: #b869a1;
	transition: .5s;
}


.scrollToTop,
.mu-testimonial-slide .slick-dots li.slick-active,
#mu-callto-action {
	background-color: #0091ea;
}

.mu-slide .slick-prev:hover,
.mu-slide .slick-prev:focus,
.mu-slide .slick-next:hover,
.mu-slide .slick-next:focus {
    color: #f67b30;
}
.mu-slide .slick-prev:hover:before,
.mu-slide .slick-prev:focus:before,
.mu-slide .slick-next:hover:before,
.mu-slide .slick-next:focus:before {
    color: #f67b30;
}

.mu-single-slide:before {
	background: #666666;
	content: '';
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	opacity: 0.3;
	z-index: 10;
}


.slick-slide img {
	margin: auto;
}

.textLink {
